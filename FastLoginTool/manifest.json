{
  "name": "登录小旋风",
  "manifest_version": 2,
  "version": "1.0",
  "description": "快速登录到商旅offline国内和海外酒店，offline国内和海外机票，1308，以及1477和1199等模块",
  "icons": {
      "128": "ctripCorp.png"
  },
  "options_page": "options.html",
  "background": {
	"page":
	"background.html"
  }, 
  "content_scripts": [
    {
      "matches": ["*://*.ctriptravel.com/*", "*://*.ctripcorp.com/*"],
      "js": ["fastToOffline.js"]
    }
  ]
}